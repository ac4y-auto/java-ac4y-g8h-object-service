<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>ac4y</groupId>
	<artifactId>ac4y-g8h-object-service</artifactId>
	<version>1.0.20260206.1</version>

	<name>java-ac4y-g8h-object-service</name>
	<description>Object service layer with request/response DTOs for G8H GUID-to-HumanId operations</description>
	<url>https://github.com/ac4y-auto/java-ac4y-g8h-object-service</url>

	<repositories>
		<repository>
			<id>github</id>
			<name>GitHub Packages</name>
			<url>https://maven.pkg.github.com/ac4y-auto/*</url>
			<snapshots><enabled>true</enabled></snapshots>
		</repository>
	</repositories>

	<distributionManagement>
		<repository>
			<id>github</id>
			<name>GitHub Packages</name>
			<url>https://maven.pkg.github.com/ac4y-auto/java-ac4y-g8h-object-service</url>
		</repository>
	</distributionManagement>

	<build>
		<sourceDirectory>src/main/java</sourceDirectory>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>2.22.2</version>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<version>3.2.0</version>
				<configuration>
					<archive>
						<manifest>
							<addDefaultImplementationEntries>true</addDefaultImplementationEntries>
							<addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
						</manifest>
					</archive>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>3.2.1</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>3.2.0</version>
				<executions>
					<execution>
						<id>attach-javadocs</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-base</artifactId>
			<version>1.1.20260206.2</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-base-for-json-and-xml</artifactId>
			<version>1.0.20260207.1</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-g8h-basic</artifactId>
			<version>1.0.20260206.1</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-guid-and-human-id</artifactId>
			<version>1.0.20260206.1</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-log-basic</artifactId>
			<version>1.0.20260206.1</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-log-object-service</artifactId>
			<version>1.0.20260206.1</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-service-common</artifactId>
			<version>1.0.20260206.3</version>
		</dependency>
		<dependency>
			<groupId>ac4y</groupId>
			<artifactId>ac4y-service-domain</artifactId>
			<version>1.0.20260206.3</version>
		</dependency>
	</dependencies>

</project>
